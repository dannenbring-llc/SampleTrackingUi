                                    @Html.LabelFor(m => m.FreezerLocationVerifySlot, "Drawer Slot:")
                                    @Html.TextBoxFor(m => m.FreezerLocationVerifySlot, new { type = "text", @class = "form-control input-sm PostBackOnChange", @style = "text-transform:uppercase" })
                                    <div class="form-group" style="margin-left: 10px">
                                        <button type="submit" name="ValidateFreezerLocation" value="true" class="btn btn-primary">Validate Location</button>
                                    </div>

                                </div>
                            </div>

                            <div id="freezerLocationVerifyDiv" style="display: none; border-top: 15px">

                                <div class="form-group; center-block;" style="width: 100%; margin-top: 2px">
                                    @Html.LabelFor(m => m.FreezerLocationVerifyDrawerId, "Drawer ID:")
                                    @Html.TextBoxFor(m => m.FreezerLocationVerifyDrawerId, new { type = "text", @class = "form-control input-sm", @style = "text-transform:uppercase" })
                                    @Html.LabelFor(m => m.FreezerLocationVerifySlot, "Drawer Slot:")
                                    @Html.TextBoxFor(m => m.FreezerLocationVerifySlot, new { type = "text", @class = "form-control input-sm", @style = "text-transform:uppercase" })
                                    <div class="form-group" style="margin-left: 10px">
                                        <button type="submit" name="ValidateFreezerLocation" value="true" class="btn btn-primary">Validate Location</button>
                                    </div>

                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
                    }
                }
                if (Model.FreezerLocationMatches && Model.FreezerLocationValidate)
                {
            <div id="closeSessionDiv" style="display: block; border-top: 15px">
                @if (Model.UnknownAliquots.Count == 0)
                        {
                <button type="submit" name="CloseSession" value="true" class="btn btn-success">Close Session</button>
                        }

            </div>
                }
                else if (Model.FreezerLocationValidate)
                {
            <div class="alert alert-warning" role="alert" style="margin-top: 7px">
                Freezer/Drawer/DrawerSlot do not match.<br />
            </div>
                }

            <input name="FreezerId" type="hidden" />
            <input name="DrawerId" type="hidden" />
            <input name="DrawerSlotId" type="hidden" />
            }
            else if (Model.Session == null)
            {
            <div class="alert alert-warning" role="alert" style="margin-top: 7px">
                No open session exists.
            </div>
            }
            else
            {
            <div class="alert alert-warning" role="alert" style="margin-top: 7px">

                <h1>
                    No scan found for existing session/tray. <br />
                </h1>
                <h2>
                    <ul>
                        <li>Confirm that Hamilton ID program is open</li>
                        <li>Place tray on scanner bed with barcode facing in</li>
                        <li>Press Blue button</li>
                        <li>Wait for screen to populate with tray map</li>
                    </ul>
                </h2>
                <h3>
                    <div style="font-size:larger" id="countdown"></div>
                    <br />
                    <meta http-equiv="refresh" content="6">
                </h3>
            </div>
            }
        </div>
        <input hidden="hidden" id="trayName" value=@Model.ScanData.TrayCode />
    </div>
</form>

<script type="text/javascript">
    var timeleft = 5;
    var downloadTimer = setInterval(function () {
        document.getElementById("countdown").innerHTML = timeleft + " seconds remaining until refresh";
        timeleft -= 1;
        if (timeleft <= 0) {
            clearInterval(downloadTimer);
            //        document.getElementById("countdown").innerHTML = "Finished"
        }
    }, 1000);
</script>